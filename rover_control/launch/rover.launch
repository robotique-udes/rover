<!-- Full rover launch / Rover side -->
<launch>
    <!-- rover_control -->
    <include file="$(find rover_control)/launch/motor_drivers.launch"/>

    <!-- Launch twist to motor_cmd node, this is also where the watchdog is -->
    <node pkg="rover_control" name="twist2motor_cmd" type="twist2motor_cmd.py" output="screen"/>

    <include file="$(find rover_control)/launch/camera_control.launch"/>


    <!-- Relay board -->
    <arg name="relay_board_port" default="/dev/serial/by-id/usb-Devantech_Ltd._USB-RLY16L._00055872-if00" />

	<node type="rly_08_node" name="rly_08_node" pkg="rly_08" output="screen">
		<param name="port" value="$(arg relay_board_port)" />
		<!-- <param name="port"
		value="/dev/serial/by-id/usb-Devantech_Ltd._USB-RLY16L._00055872-if00"
			type="string" /> -->
		<param name="outputs" value="8" type="int" />
	</node>

</launch>
